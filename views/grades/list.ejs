<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Grades</title>
</head>

<body>
    <h1>Student Grades</h1>

    <table>
        <thead>
            <tr>
                <th></th>
            </tr>
        </thead>

        <tbody>
            <% grades.forEach(function(grade) { %>
                <tr>
                    <td>
                        <%= grade.id %>
                    </td>
                </tr>
                <% }) %>
        </tbody>
    </table>
</body>

</html>